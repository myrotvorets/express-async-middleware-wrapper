{
  "name": "@myrotvorets/express-async-middleware-wrapper",
  "version": "2.2.1",
  "description": "Wrapper for Express.js async middleware to handle rejected promises and synchronous exceptions",
  "main": "dist/lib/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint -f gha .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "prepack": "npm run build",
    "test": "node --require=ts-node/register/transpile-only test-runner.ts",
    "test:coverage": "c8 npm test"
  },
  "files": [
    "dist/lib/index.d.ts",
    "dist/lib/index.js",
    "dist/lib/index.js.map"
  ],
  "typings": "dist/lib/index.d.ts",
  "keywords": [
    "express",
    "async",
    "await",
    "middleware",
    "handle rejected promises",
    "express middleware"
  ],
  "author": "Myrotvorets <support@myrotvorets.center> (https://myrotvorets.center/)",
  "license": "MIT",
  "devDependencies": {
    "@myrotvorets/eslint-config-myrotvorets-ts": "^2.27.4",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.19.5",
    "@types/supertest": "^6.0.2",
    "c8": "^10.1.2",
    "eslint-formatter-gha": "^1.5.1",
    "express": "^5.0.0",
    "node-reporter-gha": "^2.0.4",
    "node-reporter-sonarqube": "^1.0.1",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "express": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myrotvorets/express-async-middleware-wrapper.git"
  },
  "bugs": {
    "url": "https://github.com/myrotvorets/express-async-middleware-wrapper/issues"
  },
  "homepage": "https://github.com/myrotvorets/express-async-middleware-wrapper#readme",
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
